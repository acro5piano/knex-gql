{
  "name": "knex-gql",
  "version": "0.0.5-dev",
  "description": "A Schema First GraphQL Query Builder for Knex.js",
  "main": "dist/index.js",
  "repository": "git@github.com:acro5piano/knex-gql",
  "scripts": {
    "r": "ts-node -T",
    "prepare": "rm -f node_modules/graphql-fields-list/index.ts",
    "prepublish": "yarn build",
    "pgcli": "pgcli postgres://postgres:postgres@127.0.0.1:11155/postgres",
    "test": "ava --serial",
    "build": "rimraf dist && tsc -p tsconfig.prod.json"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@graphql-tools/schema": "^7.1.5",
    "dataloader": "^2.0.0",
    "graphql-fields-list": "^2.2.4",
    "graphql-scalars": "^1.10.0",
    "mem": "^8.1.1"
  },
  "peerDependencies": {
    "graphql": "^15"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.3.3",
    "@types/uuid": "^8.3.1",
    "ava": "^3.15.0",
    "dayjs": "^1.10.6",
    "delay": "^5.0.0",
    "graphql": "^15.5.1",
    "import-sort-style-module": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "knex-little-logger": "^0.0.2",
    "knex-stringcase": "^1.4.5",
    "mockdate": "^3.0.5",
    "pg": "^8.6.0",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "remeda": "^0.0.29",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
